pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                echo 'This is the checkout stage'
            }
        }
        stage('build') {
            steps {
                echo 'This is the build stage'
            }
        }
        stage('test') {
            steps {
                echo 'This is the test stage'
            }
        }
        stage('deploy') {
            steps {
                echo 'This is the deploy stage'
            }
        }
    }
}
